// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int    @id @default(autoincrement())
  name        String
  alt         String
  price       Int    @default(60)
  description String
  image       String
  category    String
}
